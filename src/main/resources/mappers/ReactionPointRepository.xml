<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.example.demo.repository.ReactionPointRepository">





	<select id="getReactionPointCntByArticleId" parameterType="int"
		resultType="int">
		SELECT COUNT(*)
		FROM reactionPoint
		WHERE relTypeCode =
		'article' AND relId =${articleId} AND `point` = ${type}


	</select>


</mapper>